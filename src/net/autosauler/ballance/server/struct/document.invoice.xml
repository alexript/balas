<?xml version="1.0" encoding="UTF-8"?>
<struct access="ADMIN|MANAGER|FINANCES">
	<names>
		<en>Invoices</en>
		<ru>Выставленные счета</ru>
	</names>
	<fields>
		<field name="partner" type="DT_CATALOGRECORD">
			<default>0</default>
			<names>
				<en>Partner</en>
				<ru>Контрагент</ru>
			</names>
			<inlist>true</inlist>
			<width>100</width>
			<helper type="catalog">partners</helper>
		</field>
		<field name="cargo" type="DT_DOCUMENTRECORD">
			<default>0</default>
			<names>
				<en>Cargo document</en>
				<ru>Документ-основание (доставка)</ru>
			</names>
			<width>100</width>
			<helper type="document">cargo</helper>
		</field>
	</fields>
	<tables>
		<table name="invoicerecords">
			<names>
				<en>Records</en>
				<ru>Записи доставки</ru>
			</names>
		</table>
	</tables>

</struct>