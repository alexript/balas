<?xml version="1.0" encoding="UTF-8"?>
<struct access="ADMIN|DOCUMENTS|FINANCES">
	<names>
		<en>Cargo</en>
		<ru>Доставка груза</ru>
	</names>
	<fields>

		<field name="citydate" type="DT_DATE">
			<default></default>
			<names>
				<en>City date</en>
				<ru>Дата прибытия в город</ru>
			</names>
			<inlist>true</inlist>
			<width>100</width>
		</field>

		<field name="cars" type="DT_CATALOGRECORD">
			<default>0</default>
			<names>
				<en>Car number</en>
				<ru>N а/м</ru>
			</names>
			<inlist>true</inlist>
			<width>50</width>
			<helper type="catalog">cars</helper>
		</field>
		<field name="drivers" type="DT_CATALOGRECORD">
			<default>0</default>
			<names>
				<en>Driver</en>
				<ru>Водитель</ru>
			</names>
			<inlist>true</inlist>
			<width>100</width>
			<helper type="catalog">drivers</helper>
		</field>
		<field name="status" type="DT_CATALOGRECORD">
			<default></default>
			<names>
				<en>State</en>
				<ru>Статус</ru>
			</names>
			<inlist>true</inlist>
			<width>70</width>
			<helper type="catalog">shipstatus</helper>
		</field>
		<field name="statusgdate" type="DT_DATE">
			<default></default>
			<names>
				<en>Status date</en>
				<ru>Дата статуса</ru>
			</names>
			<inlist>true</inlist>
			<width>100</width>
		</field>
	</fields>
	<tables>
		<table name="ingoods">
			<names>
				<en>Goods</en>
				<ru>Грузы</ru>
			</names>
		</table>
		<table name="cargoaddpay">
			<names>
				<en>Additional payments</en>
				<ru>Дополнительные расходы</ru>
			</names>
		</table>
	</tables>

</struct>